{
	"all": [
		{
			"name": "lint",
			"cmd": "bun run lint",
			"dependsOn": ["format"]
		},
		{
			"name": "format",
			"cmd": "bun run format"
		},
		{
			"name": "typecheck",
			"cmd": "bun run typecheck",
			"dependsOn": ["format"]
		},
		{
			"name": "build",
			"cmd": "bun run build",
			"dependsOn": ["lint"]
		},
		{
			"name": "test",
			"cmd": "bun run test",
			"dependsOn": ["lint"]
		}
	]
}
